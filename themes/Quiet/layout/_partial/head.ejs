<link rel="shortcut icon" type="image/x-icon" href="<%- theme.icon %>">
<%- css('css/index.css') %>
<meta property="og:description" content="<%- page.excerpt %>">
<meta property="og:image" content="https://gulinga.github.io/image/logo.png">
