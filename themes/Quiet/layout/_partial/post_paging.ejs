<div class="post-paging">
  <%let curIndex = page.index%> <%let prev = page.prev%> <%while(prev &&
  prev.index != curIndex) {%> <%prev = prev.prev%> <%}%> <% if(prev) {%>
  <a href="<%- url_for(prev.path) %>">
    <div class="post-paging-last">
      <span>上一篇</span>
      <p><%= prev.title %></p>
    </div>
  </a>
  <% } %> <%let next = page.next%> <%while(next && next.index != curIndex) {%>
  <%next = next.next%> <%}%> <% if(next) {%>
  <a href="<%- url_for(next.path) %>">
    <div class="post-paging-next">
      <span>下一篇</span>
      <p><%= next.title %></p>
    </div>
  </a>
  <% } %>
</div>
